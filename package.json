{
  "name": "blockstack.ts",
  "version": "0.16.1",
  "description": "JavaScript / TypeScript library for interacting with Blockstack",
  "author": "vsund",
  "license": "MIT",
  "homepage": "https://github.com/ntzwrk/blockstack.ts#readme",
  "repository": "git+https://github.com/ntzwrk/blockstack.ts.git",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "blockstack"
  ],
  "scripts": {
    "lint": "tslint --project ./",
    "build": "tsc",
    "prepublishOnly": "yarn run build",
    "clean": "rm -r ./dist/",
    "test": "yarn run build && mocha --require ts-node/register 'tests/**/*.spec.ts'",
    "document": "typedoc --out ./docs/code/ ./src/ --sourcefile-url-prefix https://github.com/ntzwrk/blockstack.ts/blob/master/src/"
  },
  "dependencies": {
    "@types/node": "^9.4.7",
    "bigi": "^1.4.2",
    "bitcoinjs-lib": "^3.3.2",
    "cheerio": "^1.0.0-rc.2",
    "custom-protocol-detection-blockstack": "^1.1.3",
    "ecurve": "^1.0.6",
    "elliptic": "^6.4.0",
    "form-data": "^2.3.2",
    "hasprop": "^0.0.4",
    "isomorphic-fetch": "^2.2.1",
    "jsontokens": "^0.7.7",
    "query-string": "^6.0.0",
    "schema-inspector": "^1.6.8",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2",
    "uri-js": "^3.0.2",
    "zone-file": "^0.2.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "tslint": "^5.9.1",
    "tslint-sonarts": "^1.6.0",
    "typedoc": "^0.11.1",
    "typedoc-plugin-sourcefile-url": "^1.0.3"
  }
}
